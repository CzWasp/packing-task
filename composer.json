{
    "name": "shipmonk/packing",
    "description": "Testing day application stub",
    "require": {
        "ext-pdo": "*",
        "ext-json": "*",
        "php": ">=8.4",
        "psr/http-message": "2.*",
        "guzzlehttp/psr7": "2.*",
        "doctrine/orm": "3.*",
        "symfony/cache": "7.*",
        "guzzlehttp/guzzle": "^7.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "tests/"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "slevomat/coding-standard": "^8.19",
        "phpunit/phpunit": "^12.2"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
